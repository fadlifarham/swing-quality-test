{
	"name": "boilerplate-express-typescript",
	"version": "0.0.3",
	"main": "dist/server.js",
	"scripts": {
		"start": "node dist/server.js",
		"start-dev": "node dist/server.js",
		"build": "tsc",
		"dev": "./node_modules/.bin/concurrently \"tsc -w\" \"nodemon\"",
		"clean": "rimraf dist",
		"update:all": "node dist/scripts/updateScript.js",
		"prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
		"migrate": "yarn clean && yarn build && ./node_modules/.bin/sequelize db:migrate",
		"seed": "yarn clean && yarn build && ./node_modules/.bin/sequelize db:seed:all",
		"migrate:nobuild": "./node_modules/.bin/sequelize db:migrate",
		"seed:nobuild": "./node_modules/.bin/sequelize db:seed:all",
		"generate:migration": "ts-node ./scripts/generateMigration",
		"generate:seeder": "ts-node ./scripts/generateSeeder",
		"db:create": "yarn build && sequelize db:create",
		"db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
		"db:drop": "yarn build && sequelize db:drop",
		"db:reset": "./node_modules/.bin/sequelize db:drop && ./node_modules/.bin/sequelize db:create && yarn migrate && yarn seed",
		"test": "cross-env NODE_ENV=test mocha './tests/**/?(*.)+(spec|test).[jt]s?(x)'",
		"pretest": "cross-env NODE_ENV=test yarn db:reset",
		"test:single": "cross-env NODE_ENV=test mocha",
		"export:Form": "node ./dist/scripts/exportForm.js",
		"import:Form": "node ./dist/scripts/importForm.js",
		"migrate:form": "node ./dist/scripts/migrateForm.js"
	},
	"repository": "git@gitlab.com:mv-lanius/products/service0/boiler-plate-be-ts.git",
	"author": "Machine Vision <www.machinevision.global>",
	"license": "MIT",
	"dependencies": {
		"@types/clear": "^0.1.1",
		"@types/figlet": "^1.2.1",
		"@types/inquirer": "^7.3.1",
		"@types/mkdirp": "^1.0.1",
		"axios": "^0.21.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"chalk": "^4.1.0",
		"clear": "^0.1.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.5",
		"exceljs": "^4.2.0",
		"exif": "^0.6.0",
		"express": "^4.17.1",
		"express-static-gzip": "^2.1.1",
		"express-validator": "^6.6.0",
		"figlet": "^1.5.0",
		"get-video-duration": "^3.0.2",
		"html-pdf": "^2.2.0",
		"inquirer": "^7.3.3",
		"jimp": "^0.16.1",
		"jsonwebtoken": "^8.5.1",
		"jszip": "^3.5.0",
		"lodash": "^4.17.15",
		"moment": "^2.27.0",
		"mongoose": "5.10.11",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"pdf-parse": "^1.1.1",
		"pg": "^8.4.1",
		"pg-hstore": "^2.3.3",
		"reflect-metadata": "^0.1.13",
		"remove.bg": "^1.3.0",
		"sequelize": "^6.3.5",
		"swagger-ui-express": "^4.1.4",
		"winston": "^3.3.3",
		"xendit-node": "^1.8.0",
		"yargs": "^15.4.0"
	},
	"devDependencies": {
