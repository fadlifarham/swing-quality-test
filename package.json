		"@types/cors": "^2.8.8",
		"@types/dotenv": "^8.2.0",
		"@types/ejs": "^3.0.5",
		"@types/eslint": "^7.2.0",
		"@types/express": "^4.17.6",
		"@types/html-pdf": "^2.2.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/lodash": "^4.14.157",
		"@types/mocha": "^8.0.4",
		"@types/mongoose": "^5.7.37",
